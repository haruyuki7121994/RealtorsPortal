@using Microsoft.AspNetCore.Http
@inject Microsoft.AspNetCore.Http.IHttpContextAccessor HttpContextAccessor
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - src</title>
    <!-- Font awesome -->
    <link href="//maxcdn.bootstrapcdn.com/font-awesome/4.1.0/css/font-awesome.min.css" rel="stylesheet">
    <!-- Bootstrap -->
    <link href="~/css/RealtorsPortal/bootstrap.css" rel="stylesheet">
    <!-- slick slider -->
    <link rel="stylesheet" type="text/css" href="~/css/RealtorsPortal/slick.css">
    <!-- price picker slider -->
    <link rel="stylesheet" type="text/css" href="~/css/RealtorsPortal/nouislider.css">
    <!-- Fancybox slider -->
    <link rel="stylesheet" href="~/css/RealtorsPortal/jquery.fancybox.css" type="text/css" media="screen" />
    <!-- Theme color -->
    <link id="switcher" href="~/css/RealtorsPortal/theme-color/default-theme.css" rel="stylesheet">

    <!-- Main style sheet -->
    <link href="~/css/RealtorsPortal/style.css?version=2.2" rel="stylesheet">

    <!-- Google Font -->
    <link href='https://fonts.googleapis.com/css?family=Vollkorn' rel='stylesheet' type='text/css'>
    <link href='https://fonts.googleapis.com/css?family=Open+Sans' rel='stylesheet' type='text/css'>

</head>
<body class="aa-price-range">
    <!-- Pre Loader -->
    <div id="aa-preloader-area">
        <div class="pulse"></div>
    </div>
    <!-- SCROLL TOP BUTTON -->
    <a class="scrollToTop" href="#"><i class="fa fa-angle-double-up"></i></a>
    <!-- END SCROLL TOP BUTTON -->
    <!-- Start header section -->
    <header id="aa-header">
        <div class="container">
            <div class="row">
                <div class="col-md-12">
                    <div class="aa-header-area">
                        <div class="row">
                            <div class="col-md-6 col-sm-6 col-xs-6">
                                <div class="aa-header-left">
                                    <div class="aa-telephone-no">
                                        <span class="fa fa-phone"></span>
                                        1-900-523-3564
                                    </div>
                                    <div class="aa-email hidden-xs">
                                        <span class="fa fa-envelope-o"></span> info@markups.com
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-6 col-sm-6 col-xs-6">
                                @if (HttpContextAccessor.HttpContext.Session.GetString("customer") != null)
                                {
                                    <div class="aa-header-right">
                                        @Html.ActionLink("Profile", "Index", "Customer", new { }, new { @class = "aa-register" })
                                        @Html.ActionLink("Logout", "Logout", "Auth", new { }, new { @class = "aa-login" })
                                    </div>
                                }
                                else
                                {
                                    <div class="aa-header-right">
                                        @Html.ActionLink("Register", "Register", "Auth", new { }, new { @class = "aa-register" })
                                        @Html.ActionLink("Login", "Login", "Auth", new { }, new { @class = "aa-login" })
                                    </div>
                                }
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </header>
    <!-- End header section -->
    <!-- Start menu section -->
    <section id="aa-menu-area">
        <nav class="navbar navbar-default main-navbar" role="navigation">
            <div class="container">
                <div class="navbar-header">
                    <!-- FOR MOBILE VIEW COLLAPSED BUTTON -->
                    <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
                        <span class="sr-only">Toggle navigation</span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                    <!-- LOGO -->
                    <!-- Text based logo -->
                    <a class="navbar-brand aa-logo" href="index.html"> Home <span>Property</span></a>
                    <!-- Image based logo -->
                    <!-- <a class="navbar-brand aa-logo-img" href="index.html"><img src="img/logo.png" alt="logo"></a> -->
                </div>
                <div id="navbar" class="navbar-collapse collapse">
                    <ul id="top-menu" class="nav navbar-nav navbar-right aa-main-nav">
                        <li class="active">@Html.ActionLink("HOME", "Index", "Home", new { /* id=item.PrimaryKey */ })</li>
                        <li>@Html.ActionLink("PROPERTIES", "Properties", "Home", new { /* id=item.PrimaryKey */ })</li>
                        <li>@Html.ActionLink("NEWS", "Blogs", "Home", new { /* id=item.PrimaryKey */ })</li>
                        <li>@Html.ActionLink("CONTACT", "Contact", "Home", new { /* id=item.PrimaryKey */ })</li>
                    </ul>
                </div><!--/.nav-collapse -->
            </div>
        </nav>
    </section>
    <!-- End menu section -->

    @RenderBody()

    <!-- Footer -->
    <footer id="aa-footer">
        <div class="container">
            <div class="row">
                <div class="col-md-12">
                    <div class="aa-footer-area">
                        <div class="row">
                            <div class="col-md-3 col-sm-6 col-xs-12">
                                <div class="aa-footer-left">
                                    <p>Designed by <a rel="nofollow" href="http://www.markups.io/">MarkUps.io</a></p>
                                </div>
                            </div>
                            <div class="col-md-3 col-sm-6 col-xs-12">
                                <div class="aa-footer-middle">
                                    <a href="#"><i class="fa fa-facebook"></i></a>
                                    <a href="#"><i class="fa fa-twitter"></i></a>
                                    <a href="#"><i class="fa fa-google-plus"></i></a>
                                    <a href="#"><i class="fa fa-youtube"></i></a>
                                </div>
                            </div>
                            <div class="col-md-6 col-sm-12 col-xs-12">
                                <div class="aa-footer-right">
                                    <a href="#">Home</a>
                                    <a href="#">Support</a>
                                    <a href="#">License</a>
                                    <a href="#">FAQ</a>
                                    <a href="#">Privacy & Term</a>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </footer>
    <!-- / Footer -->
    <!-- jQuery library -->
    <!--<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js"></script> -->
    <script src="~/js/RealtorsPortal/jquery.min.js"></script>
    <!-- Include all compiled plugins (below), or include individual files as needed -->
    <script src="~/js/RealtorsPortal/bootstrap.js"></script>
    <!-- slick slider -->
    <script type="text/javascript" src="~/js/RealtorsPortal/slick.js"></script>
    <!-- Price picker slider -->
    <script type="text/javascript" src="~/js/RealtorsPortal/nouislider.js"></script>
    <!-- mixit slider -->
    <script type="text/javascript" src="~/js/RealtorsPortal/jquery.mixitup.js"></script>
    <!-- Add fancyBox -->
    <script type="text/javascript" src="~/js/RealtorsPortal/jquery.fancybox.pack.js"></script>
    <!-- CKeditor -->
    <script type="text/javascript" src="~/lib/ckeditor/ckeditor.js"></script>
    <!-- Custom js -->
    <script src="~/js/RealtorsPortal/custom.js"></script>

    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
